{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/main.py"
    }
  ],
  "functions": {
    "api/main.py": {
      "runtime": "python3.9",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
